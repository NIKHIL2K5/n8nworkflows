      "name": "Sticky Note1"
    },
    {
      "parameters": {
        "command": "=# Set folder containing clips\n$folder = \"C:\\n8n\\videos\"   # <--- CHANGE IF NEEDED\nSet-Location $folder\n\n# Get all videoclip_*.mp4 files sorted\n$inputs = Get-ChildItem -Filter \"videoclip_*.mp4\" | Sort-Object Name\n\nif ($inputs.Count -lt 2) {\n    Write-Host \"Need at least 2 clips\"\n    exit 1\n}\n\n# Build input arguments\n$inputArgs = \"\"\n$index = 0\nforeach ($clip in $inputs) {\n    $inputArgs += \" -i `\"$($clip.FullName)`\"\"\n    $index++\n}\n\n# Collect durations (rounded)\n$durations = @()\nforeach ($clip in $inputs) {\n    $d = ffprobe -v error -show_entries format=duration -of default=noprint_wrappers=1:nokey=1 \"$($clip.FullName)\"\n    $dur = [math]::Round([double]$d)\n    $durations += $dur\n}\n\n# Build filter_complex for fade transitions\n$filter = \"\"\n$last = \"[0:v]\"\n$transition = \"fade\"   # change to smoothleft / slideright / etc.\n$duration = 1          # fade duration in seconds\n$offset = 0\n$outIndex = 1\n\nfor ($i = 0; $i -lt $durations.Count - 1; $i++) {\n\n    $next = \"[$($i+1):v]\"\n    $outv = \"[v$outIndex]\"\n\n    $clipDur = $durations[$i]\n    $offset += ($clipDur - $duration)\n\n    $filter += \"$last$next xfade=transition=$transition:duration=$duration:offset=$offset $outv;\"\n    \n    $last = $outv\n    $outIndex++\n}\n\n# Remove last semicolon\n$filter = $filter.TrimEnd(\";\")\n\n# Output final video path\n$output = \"C:\\n8n\\videos\\combinedClipsSmooth.mp4\"\n\n# Build final ffmpeg command\n$cmd = \"ffmpeg -y $inputArgs -filter_complex `\"$filter`\" -map $last -c:v libx264 -crf 23 -preset veryfast `\"$output`\"\"\n\nWrite-Host \"Running:\"\nWrite-Host $cmd\n\n# Run the command\nInvoke-Expression $cmd\n"
      },
